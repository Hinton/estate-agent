{
    "name": "laravel-enso/enso",
    "description": "Laravel Enso can be a solid start for any SPA based on Laravel 5.6, Vue and Bulma",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": "2.8.0",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "6.3.3",
        "laravel-enso/core": "3.2.1",
        "laravel-enso/addressesmanager": "2.5.6",
        "laravel-enso/commentsmanager": "2.4.8",
        "laravel-enso/companies": "1.0.9",
        "laravel-enso/contacts": "2.3.5",
        "laravel-enso/controlpanelapi": "2.0.12",
        "laravel-enso/dataimport": "2.5.3",
        "laravel-enso/discussions": "1.1.7",
        "laravel-enso/documentsmanager": "2.4.4",
        "laravel-enso/examples": "1.0.10",
        "laravel-enso/howtovideos": "2.2.3",
        "laravel-enso/teams": "1.0.11",
        "laravel/framework": "5.7.11",
        "laravel/tinker": "~1.0",
        "sentry/sentry-laravel": "dev-laravel-57@dev"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.2.0",
        "codedungeon/phpunit-result-printer": "0.19.14",
        "filp/whoops": "2.3.1",
        "fzaninotto/faker": "1.8.0",
        "laracasts/generators": "dev-develop",
        "mockery/mockery": "1.2.0",
        "nunomaduro/collision": "2.1.0",
        "phpunit/phpunit": "7.4.3",
        "wnx/laravel-stats": "1.8.0"
    },
    "repositories": [{
        "type": "git",
        "url": "https://github.com/bogdankharchenko/Laravel-5-Generators-Extended"
    }],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --force --tag=enso-assets",
            "php artisan vendor:publish --tag=localisation-lang-files",
            "php artisan enso:localisation:merge"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
